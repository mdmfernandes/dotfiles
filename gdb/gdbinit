# pwndbg
source ~/.local/src/pwndbg/gdbinit.py

set show-flags on
#set context-clear-screen on

# History
set history save on
set history size unlimited
set history remove-duplicates unlimited
set history filename ~/.gdb_history

# TMUX
source ~/.local/src/pwndbgtmux.py

# GEP
# Make sure the following line is always the last in this file
source ~/.local/src/GEP/gdbinit-gep.py
